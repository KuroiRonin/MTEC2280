
// Size(width,height): This function creates a window.
size(500,500);

/* background(brightness)
   background(red,blue,green)
   This function sets the background color of the window
*/
background(200,175,255);

/* fill(brightness)
   fill(red,blue,green)
   This function sets the color of the following object
*/

fill(255,125,0);

// Stroke works like fill
stroke(0,175 ,0);

// noStroke() disables the stroke of your object

// rect(left corner x, left corner y, width,height)
// This function creates a rectangle with the given info

rect(155,155,155,155,150);

// ellipse(center x, center y, width,height)
// This function creates a circle with the given info
ellipse(100,100,100,100);
